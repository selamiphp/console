{
    "name": "selami/console",
    "description": "Creating Console Application library on top of Symfony Console",
    "type": "library",
    "require": {
        "php": "^7.1",
        "selami/stdlib": "^1.6",
        "symfony/console": "^4.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "zendframework/zend-servicemanager": "^3.3",
        "php-coveralls/php-coveralls": "~1.0",
        "phpunit/phpcov": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mehmet Korkmaz",
            "email": "mehmet@mkorkmaz.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Selami\\Console\\": "src/Console"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SelamiConsoleTest\\": "tests/resources/"
        }
    }
}
